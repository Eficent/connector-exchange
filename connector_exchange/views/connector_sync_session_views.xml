<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_connector_sync_session_tree" model="ir.ui.view">
      <field name="name">connector.sync.session.tree</field>
      <field name="model">connector.sync.session</field>
      <field name="inherit_id" ref="connector_sync_session.view_connector_sync_session_tree"/>
      <field name="arch" type="xml">
        <field name="state" position="before">
          <field name="sync_exchange_import"/>
          <field name="sync_exchange_export"/>
          <field name="sync_calendar_exchange_import"/>
          <field name="sync_calendar_exchange_export"/>
        </field>
      </field>
    </record>

    <record id="view_connector_sync_session_form" model="ir.ui.view">
      <field name="name">connector.sync.session.form</field>
      <field name="model">connector.sync.session</field>
      <field name="inherit_id" ref="connector_sync_session.view_connector_sync_session_form"/>
      <field name="arch" type="xml">
        <group name="backends" position="inside">
          <field name="sync_exchange_import"/>
          <field name="sync_exchange_export"/>
          <field name="sync_calendar_exchange_import"/>
          <field name="sync_calendar_exchange_export"/>
          <field name="exchange_backend_ids"/>
        </group>
      </field>
    </record>

    <record id="view_connector_sync_session_search" model="ir.ui.view">
      <field name="name">connector.sync.session.search</field>
      <field name="model">connector.sync.session</field>
      <field name="inherit_id" ref="connector_sync_session.view_connector_sync_session_search"/>
      <field name="arch" type="xml">
        <field name="state" position="after">
          <field name="sync_exchange_import"/>
          <field name="sync_exchange_export"/>
          <field name="sync_calendar_exchange_import"/>
          <field name="sync_calendar_exchange_export"/>
        </field>
      </field>
    </record>

</odoo>
